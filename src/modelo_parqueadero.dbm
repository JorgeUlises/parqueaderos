	<columns names="id_propietario" ref-type="src-columns"/>
	<columns names="id" ref-type="dst-columns"/>
</constraint>

<constraint name="isla_vehiculo_id_vehiculo_fkey" type="fk-constr" comparison-type="MATCH FULL"
