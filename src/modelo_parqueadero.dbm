	 dst-table="public.vehiculo"
	 src-required="false" dst-required="false"/>

<relationship name="rel_isla_grupo_isla" type="relfk"
	 src-table="public.isla"
	 dst-table="public.grupo_isla"
	 src-required="false" dst-required="false"/>

</dbmodel>
